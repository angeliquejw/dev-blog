{
  "name": "angeliqueweger.com",
  "version": "2.0.0",
  "scripts": {
    "clean": "rm -rf _site && rm -rf .cache && rm -rf _build",
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "dev:parcel": "parcel watch src/assets/img/* src/assets/styles/styles.scss src/assets/scripts/index.js --out-dir _site/assets",
    "build": "run-s clean prod:*",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy --output=./_build",
    "prod:parcel": "parcel build src/assets/img/* src/assets/styles/styles.scss src/assets/scripts/index.js --out-dir _build/assets",
    "debug": "DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/angeliquejw/dev-blog.git"
  },
  "author": {
    "name": "Angelique Weger",
    "email": "hello@angeliqueweger.com",
    "url": "https://angeliqueweger.com/"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-imagemin": "^3.0.0",
    "sass": "^1.24.2",
    "stylelint-8-point-grid": "^0.2.3",
    "stylelint-order": "^4.0.0"
  }
}
