<header>
  <h2><a href="{{ '/' | url }}"><span class="u-sr-only">{{ metadata.title }}</span><span class="site-logo" aria-hidden="true" title="Angelique Weger">a</span></a></h2>

  {% include "social-list.njk" %}

  <ul class="u-list-reset">
  {%- for entry in collections.all | eleventyNavigation %}
    <li class="{% if entry.url == page.url %} --active{% endif %}"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
  {%- endfor %}
  </ul>
</header>
