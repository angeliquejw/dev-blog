<header>
  <h2 class="home"><a href="{{ '/' | url }}">{{ metadata.title }}</a></h2>

  {% include "social-list.njk" %}

  <ul>
  {%- for entry in collections.all | eleventyNavigation %}
    <li class="{% if entry.url == page.url %} --active{% endif %}"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
  {%- endfor %}
  </ul>
</header>
