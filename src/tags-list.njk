<ul class="u-list-reset">
  {% for tagKey, tagVal in collections.tagCount %}
  {% set tagUrl %}/tags/{{ tagKey | slug }}/{% endset %}
  <li><a href="{{ tagUrl | url }}">{{ tagKey }} {{ tagVal }}</a></li>
  {% endfor %}
</ul>
